<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Fira Code" />
    <option name="USE_LIGATURES" value="true" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_DND_ENABLED" value="false" />
  </component>
  <component name="SqlEditorOptions">
    <option name="aliasQualification" value="Always" />
    <option name="namespaceQualification" value="Never" />
    <option name="tableQualification" value="Never" />
  </component>
  <component name="TodoConfiguration">
    <multiLine>false</multiLine>
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
    <pattern icon="important" case-sensitive="true" pattern="\bNB." />
    <pattern useCustomColors="true" pattern="`[^`]*`">
      <option name="FOREGROUND" value="91774b" />
      <option name="FONT_TYPE" value="3" />
      <option name="EFFECT_COLOR" value="3c3f41" />
    </pattern>
    <pattern pattern="\bdebug\b.?" />
    <pattern pattern="\bASB\b.?" />
    <pattern pattern="\bHP-(2897|2880|2882|2886|2962)\b" />
    <filter name="TODO">
      <pattern index="1" />
      <pattern index="5" />
      <pattern index="0" />
    </filter>
    <filter name="My Tickets">
      <pattern index="5" />
      <pattern index="6" />
    </filter>
  </component>
</application>